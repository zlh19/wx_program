/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.body{
	padding-bottom:100rpx;
}

/*导航*/
.nav{
	height:100rpx;
	line-height:100rpx;
	background:#fff;
	position:fixed;
	left:0;
	bottom:0;
	width:100%;
	display:flex;
	border-top:1rpx solid #eee;
}

.nav .nav-item{
	flex:1;
}

.nav .nav-icon{display:block;text-align:center;margin:10rpx auto 0 auto;width:48rpx;height:48rpx;background-repeat:no-repeat;background-size:contain;}
.nav .nav-text{margin-top:10rpx;line-height:1;text-align:center;font-size:24rpx;color:#373737;}

.nav .icon-index{
  background-image:url(data:image/png;base64,);
}
.nav .icon-product{
  background-image:url(data:image/png;base64,);
}
.nav .icon-find{
  background-image:url(data:image/png;base64,);
}
.nav .icon-mine{
  background-image:url(data:image/png;base64,);
}
.nav .current .icon-index{
  background-image:url(data:image/png;base64,);
}
.nav .current .icon-product{
  background-image:url(data:image/png;base64,);
}
.nav .current .icon-find{
  background-image:url(data:image/png;base64,);
}
.nav .current .icon-mine{
  background-image:url(data:image/png;base64,);
}

/* 错误提示 */
.error-tips{
  position:fixed;
  left:0;
  top:0;
  width:100%;
  height:100%;
  background:rgba(0,0,0,0);
  z-index:2048;
  display:flex;
  justify-content:center;
  align-items:center;
}

.error-tips-main{
  background:rgba(0,0,0,.8);
  padding:40rpx;
  font-size:30rpx;
  border-radius:20rpx;
  color:#fff;
}
