page{
  background:#eee
}

.body{
  padding-top:20rpx;
}


.ipt-wrap{
  height:100rpx;
  border-bottom:1px solid #ebebeb;
  display:flex;
  align-items:center;
  background:#fff;
}

.address-wrap{
  border-top:1px solid #ebebeb;
  margin-top:20rpx;
}

.ipt-wrap .ipt{
  flex:1;
  color:#333;
  font-size:30rpx;
  margin:0 80rpx 0 20rpx;
  line-height:1.5;
  vertical-align: middle;
}

.ipt-wrap .ipt-placeholder{
  color:#929292;
  font-size:28rpx;
}

.ipt-wrap .address-icon{
  width:22rpx;
  height:30rpx;
  background-image:url(data:image/png;base64,);
  background-repeat:no-repeat;
  background-size:contain;
  margin-right:40rpx;
}

.upload-wrap{
  background:#fff;
  font-size:24rpx;
  color:#929292;
  padding:24rpx 0 0 20rpx;
}

.upload-list{
  margin:22rpx 0 0 0;
  display:flex;
}

.img-wrap{
  width:120rpx;
  height:120rpx;
  overflow:hidden;
  border:1px solid #eee;
  margin-right:20rpx;
  position:relative;
  display:flex;
  justify-content:center;
  align-items:center;
}

.img-wrap .icon-close{
  position:absolute;
  right:0;
  top:0;
  width:28rpx;
  height:28rpx;
  background-size:contain;
  background-image:url(data:image/png;base64,);
}

.upload-list .upload-btn{
  width:120rpx;
  height:120rpx;
  border:1px dashed #cbcbcb;
  position:relative;
  margin-bottom:28rpx;
}


.upload-list .upload-btn:after{
  content:'';
  position:absolute;
  left:50%;
  top:50%;
  margin:-20rpx 0 0 -4rpx;
  width:8rpx;
  height:40rpx;
  background:#cbcbcb;
}

 .upload-list .upload-btn:before{
  content:'';
  position:absolute;
  left:50%;
  top:50%;
  margin:-4rpx 0 0 -20rpx;
  width:40rpx;
  height:8rpx;
  background:#cbcbcb;
} 


.submit-btn{
  width:80%;
  margin:0 auto;
  height:100rpx;
  line-height:100rpx;
  background: #F7AB00;
  border-radius: 12rpx;
  color:#fff;
  font-size:32rpx;
  text-align:center;
  margin-top:40rpx;
}
